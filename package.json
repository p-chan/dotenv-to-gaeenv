{
  "name": "dotenv-to-gaeenv",
  "version": "0.1.0",
  "description": "A CLI tool to generate gaeenv from dotenv",
  "keywords": [
    "dotenv",
    "gaeenv"
  ],
  "homepage": "https://github.com/p-chan/dotenv-to-gaeenv#readme",
  "bugs": "https://github.com/p-chan/dotenv-to-gaeenv/issues",
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/p-chan/dotenv-to-gaeenv.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dev": "ts-node ./src/index.ts",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "version:major": "standard-version -r major",
    "version:minor": "standard-version -r minor",
    "version:patch": "standard-version -r patch"
  },
  "dependencies": {
    "cac": "^6.7.1",
    "dotenv": "^15.0.0",
    "js-yaml": "^4.0.0"
  },
  "devDependencies": {
    "@stardust-configs/prettier-config": "0.1.1",
    "@stardust-configs/tsconfig": "0.1.0",
    "@types/jest": "26.0.24",
    "@types/js-yaml": "4.0.5",
    "@types/node": "14.18.23",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.8",
    "dotenv-ctrl": "0.1.1",
    "jest": "26.6.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "standard-version": "9.5.0",
    "ts-jest": "26.5.6",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
